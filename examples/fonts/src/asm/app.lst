040BDC FE FF       0324*      dw 65534 ; sourceBufferId
040BDE 00 7F BF FF 0325*      db 0x00,0x7F,0xBF,0xFF ; expanding to bytes by bit-shifting?
040BE2             0326*  ; reverse the byte order to fix endian-ness:
040BE2             0327*  ; Command 24: Reverse the order of data of blocks within a buffer
040BE2             0328*  ; VDU 23, 0, &A0, bufferId; 24, options, [valueSize;] [chunkSize;]
040BE2             0329*  ; VDU 23,0,&A0,targetBufferID%;24,4,4;
