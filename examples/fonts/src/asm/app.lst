040BCF             0313*  ; Command 14: Consolidate blocks in a buffer
040BCF             0314*  ; VDU 23, 0, &A0, bufferId; 14
040BCF 17 00 A0    0315*      db 23,0,0xA0
040BD2 FE FF       0316*      dw 65534 ; workingBufferId
040BD4 0E          0317*      db 14 ; consolidate blocks
040BD5             0318*  ; the "expand bitmap" command is:
040BD5             0319*  ; VDU 23,0,&A0,targetBufferID%;&48,2,sourceBufferId%;0,&7F,&BF,&FF
040BD5 17 00 A0    0320*      db 23,0,0xA0
040BD8 00 00       0321*  @bufferId0: dw 0x0000 ; targetBufferId
040BDA 48          0322*      db 0x48 ; given as decimal command 72 in the docs
