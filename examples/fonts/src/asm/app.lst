040BAF 32 DA 0B 04 0300*      ld (@bufferId0+2),a
040BB3 3E 17       0301*      ld a,23
040BB5 32 F0 0B 04 0302*      ld (@bufferId1+2),a
040BB9 3E 18       0303*      ld a,24
040BBB 32 E7 0B 04 0304*      ld (@bufferId2+2),a
040BBF AF          0305*      xor a
040BC0 32 F7 0B 04 0306*      ld (@height+2),a
