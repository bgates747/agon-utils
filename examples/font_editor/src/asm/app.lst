040898             0263*  ; load an rgba2222 image file to a working buffer and make it an rgba8888 bitmap in a target buffer
040898             0264*  ; inputs: bc,de image width,height ; hl = targetBufferId ; ix = file size ; iy = pointer to filename
040898             0265*  vdu_load_img_rgba2_to_8:
040898             0266*  ; backup the target buffer id and image dimensions
040898 E5          0267*      push hl
040899 D5          0268*      push de
04089A C5          0269*      push bc
04089B             0270*  ; load the rgba2 image to working buffer 65534
04089B 21 FE FF 00 0271*      ld hl,65534 ; temporary working buffer id
04089F CD A2 09 04 0272*  	call vdu_load_buffer_from_file
0408A3             0273*  ; restore the image dimensions and target buffer id
0408A3 C1          0274*      pop bc
0408A4 D1          0275*      pop de
0408A5 E1          0276*      pop hl
0408A6             0277*  ; fall through to vdu_rgba2_to_8
0408A6             0278*  
0408A6             0279*  ; https://discord.com/channels/1158535358624039014/1158536711148675072/1257757461729771771
0408A6             0280*  ; ok, so the "expand bitmap" can be used, when coupled with a "reverse" - the expanded bitmap _doesn't_ come out "right" otherwise, cos of endian-ness
0408A6             0281*  ; the "expand bitmap" command is:
0408A6             0282*  ; VDU 23,0,&A0,targetBufferID%;&48,2,sourceBufferId%;0,&7F,&BF,&FF
0408A6             0283*  ; and then to reverse the byte order to fix endian-ness:
0408A6             0284*  ; VDU 23,0,&A0,targetBufferID%;24,4,4;
0408A6             0285*  ; finally you'd need to set that buffer to be an RGBA8888 format bitmap:
0408A6             0286*  ; VDU 23,27,&20,targetBufferID%;
0408A6             0287*  ; VDU 23,27,&21,width%;height%;0
0408A6             0288*  ; -------------------------------------------------------------------
0408A6             0289*  ; inputs: bc,de image width,height ; hl = targetBufferId
0408A6             0290*  ; prerequisites: rgba2 image data loaded into workingBufferId 65534
0408A6             0291*  vdu_rgba2_to_8:
0408A6             0292*  ; load the image dimensions and buffer id parameters
0408A6 ED 43 02 09 0293*      ld (@width),bc
       04          
0408AB ED 53 04 09 0294*      ld (@height),de
       04          
0408B0 22 E7 08 04 0295*      ld (@bufferId0),hl
0408B4 22 F4 08 04 0296*      ld (@bufferId2),hl
0408B8 22 FD 08 04 0297*      ld (@bufferId1),hl
0408BC             0298*  ; clean up bytes that got stomped on by the ID loads
0408BC 3E 48       0299*      ld a,0x48
0408BE 32 E9 08 04 0300*      ld (@bufferId0+2),a
0408C2 3E 17       0301*      ld a,23
0408C4 32 FF 08 04 0302*      ld (@bufferId1+2),a
0408C8 3E 18       0303*      ld a,24
0408CA 32 F6 08 04 0304*      ld (@bufferId2+2),a
0408CE AF          0305*      xor a
0408CF 32 06 09 04 0306*      ld (@height+2),a
0408D3             0307*  ; send the vdu command strings
0408D3 21 DE 08 04 0308*      ld hl,@beg
0408D7 01 29 00 00 0309*      ld bc,@end-@beg
0408DB 5B DF       0310*      rst.lil $18
0408DD C9          0311*      ret
0408DE             0312*  @beg:
0408DE             0313*  ; Command 14: Consolidate blocks in a buffer
0408DE             0314*  ; VDU 23, 0, &A0, bufferId; 14
0408DE 17 00 A0    0315*      db 23,0,0xA0
0408E1 FE FF       0316*      dw 65534 ; workingBufferId
0408E3 0E          0317*      db 14 ; consolidate blocks
0408E4             0318*  ; the "expand bitmap" command is:
0408E4             0319*  ; VDU 23,0,&A0,targetBufferID%;&48,2,sourceBufferId%;0,&7F,&BF,&FF
0408E4 17 00 A0    0320*      db 23,0,0xA0
0408E7 00 00       0321*  @bufferId0: dw 0x0000 ; targetBufferId
0408E9 48          0322*      db 0x48 ; given as decimal command 72 in the docs
0408EA 02          0323*      db 2 ; options mask: %00000011 is the number of bits per pixel in the source bitmap
0408EB FE FF       0324*      dw 65534 ; sourceBufferId
0408ED 00 7F BF FF 0325*      db 0x00,0x7F,0xBF,0xFF ; expanding to bytes by bit-shifting?
0408F1             0326*  ; reverse the byte order to fix endian-ness:
0408F1             0327*  ; Command 24: Reverse the order of data of blocks within a buffer
0408F1             0328*  ; VDU 23, 0, &A0, bufferId; 24, options, [valueSize;] [chunkSize;]
0408F1             0329*  ; VDU 23,0,&A0,targetBufferID%;24,4,4;
0408F1 17 00 A0    0330*      db 23,0,0xA0
0408F4 00 00       0331*  @bufferId2:    dw 0x0000 ; targetBufferId
0408F6 18          0332*      db 24 ; reverse byte order
0408F7 04          0333*      db 4 ; option: Reverse data of the value size within chunk of data of the specified size
0408F8 04 00       0334*      dw 4 ; size (4 bytes)
0408FA             0335*  ; finally you'd need to set that buffer to be an RGBA8888 format bitmap:
0408FA             0336*  ; VDU 23,27,&20,targetBufferID%;
0408FA 17 1B 20    0337*      db 23,27,0x20 ; select bitmap
0408FD 00 00       0338*  @bufferId1: dw 0x0000 ; targetBufferId
0408FF             0339*  ; VDU 23,27,&21,width%;height%;0
0408FF 17 1B 21    0340*      db 23,27,0x21 ; create bitmap from buffer
040902 00 00       0341*  @width: dw 0x0000
040904 00 00       0342*  @height: dw 0x0000
040906 00          0343*      db 0x00 ; rgba8888 format
040907             0344*  @end:
040907             0345*  
040907             0346*  ; scratch variables
040907 00 00 00    0347*  bufferId0: dl 0x000000
04090A 00 00 00    0348*  bufferId1: dl 0x000000
04090D             0349*  
04090D             0350*  ; load a vdu buffer from local memory
04090D             0351*  ; inputs: hl = bufferId ; bc = length ; de = pointer to data
04090D             0352*  vdu_load_buffer:
04090D ED 43 36 09 0353*      ld (@length),bc
       04          
040912 D5          0354*      push de ; save data pointer
040913             0355*  ; send the vdu command string
040913 7D          0356*      ld a,l
040914 32 33 09 04 0357*      ld (@bufferId),a
040918 7C          0358*      ld a,h
040919 32 34 09 04 0359*      ld (@bufferId+1),a
04091D 21 30 09 04 0360*      ld hl,@cmd
040921 01 08 00 00 0361*      ld bc,@end-@cmd
040925 5B DF       0362*      rst.lil $18
040927             0363*  ; send the buffer data
040927 E1          0364*      pop hl ; pointer to data
040928 ED 4B 36 09 0365*      ld bc,(@length)
       04          
04092D 5B DF       0366*      rst.lil $18 ; send it
04092F C9          0367*      ret
040930             0368*  ; Upload data :: VDU 23, 0 &A0, bufferId; 0, length; <buffer-data>
040930 17 00 A0    0369*  @cmd:       db 23,0,0xA0
040933 00 00       0370*  @bufferId:	dw 0x0000
