04006A ED 5A       0266*   	adc hl,de ; adc the offset because we need sign flag
04006C F2 74 00 04 0267*   	jp p,@not_neg
040070 21 00 00 00 0268*   	ld hl,0 ; seek to beginning of file
040074             0269*   @not_neg:
