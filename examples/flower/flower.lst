0B1667             0041*  PRT_EN_1:     equ %00000001 ;
0B1667             0042*  
0B1667             0043*  ; Table 37. Timer Input Source Select Register
0B1667             0044*  ; Each of the 4 timers are allocated two bits of the 8-bit register
