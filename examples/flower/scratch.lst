0B1292             0030*  CLK_DIV_4:    equ %00000000 ;
0B1292             0031*  
0B1292             0032*  RST_EN_0:     equ %00000000 ; The reload and restart function is disabled.
0B1292             0033*  RST_EN_1:     equ %00000010 ; The reload and restart function is enabled.
